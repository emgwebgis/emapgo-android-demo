<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <Button
        android:id="@+id/id_move_camera"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="moveCamera"/>
    <Button
        android:id="@+id/id_easy_camera"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="easeCamera"/>
    <Button
        android:id="@+id/id_animate_camera"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="animateCamera"/>
    <com.emapgo.mapsdk.maps.MapView
        android:id="@+id/emgMapView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:emg_cameraTargetLat="39.8288916490182"
        app:emg_cameraTargetLng="116.29995507512434"
        app:emg_cameraZoom="13">
    </com.emapgo.mapsdk.maps.MapView>
</LinearLayout>
